{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "right",
        "segments": [
          {
            "type": "dotnet",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#c36ff3",
            "template": "\ue0b7\ue77f v{{ .Full }}\ue0b5"
          },
          {
            "type": "node",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#68A063",
            "template": "\ue0b7\udb80\udf99 v{{ .Full }}\ue0b5"
          },
          {
            "type": "python",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#4597db",
            "template": "\ue0b7\ue606 v{{ .Full }}\ue0b5"
          },
          {
                "foreground": "#4597db",
                "properties": {
                    "display_mode": "environment",
                    "fetch_virtual_env": true,
                    "home_enabled": true
                },
                "template": "{{ if .Venv }}\ue0b7\ue606{{ .Venv }}\ue0b5{{ end }} ",
                "type": "python"
            },
          {
            "type": "lua",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#0004ff",
            "template": "\ue0b7\ue620 v{{ .Full }}\ue0b5"
          },
          {
            "type": "ruby",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#ff0000",
            "template": "\ue0b7\ue791 v{{ .Full }}\ue0b5"
          },
          {
            "type": "java",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#ff7300",
            "template": "\ue0b7\udb82\udf37 v{{ .Full }}\ue0b5"
          },
          {
            "type": "angular",
            "style": "plain",
            "properties": {
              "display_mode": "files"
            },
            "foreground": "#ff0000",
            "template": "\ue0b7\udb81\udeb2 v{{ .Full }}\ue0b5"
          },
          {
            "type": "battery",
            "style": "plain",
            "foreground": "#ffffff",
            "template": "\ue0b7{{ if not .Error }}{{ .Icon }} {{ .Percentage }}%{{ else }}\udb85\udc25{{ end }}\ue0b5",
            "properties": {
              "discharging_icon": "\uf242 ",
              "charging_icon": "\udb80\udc84 ",
              "charged_icon": "\uf240 "
            }
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "background": "#fec810",
            "foreground": "#000000",
            "properties": {
              "alpine": "\uf300",
              "arch": "\uf303",
              "centos": "\uf304",
              "debian": "\uf306",
              "elementary": "\uf309",
              "fedora": "\uf30a",
              "gentoo": "\uf30d",
              "linux": "\ue712",
              "macos": "\ue711",
              "manjaro": "\uf312",
              "mint": "\uf30f",
              "opensuse": "\uf314",
              "raspbian": "\uf315",
              "ubuntu": "\uf31c",
              "windows": "\udb81\udc1d"
            },
            "style": "diamond",
            "leading_diamond": "\ue0b2",
            "template": "{{ .Icon }} ",
            "type": "os"
          },
          {
            "background": "#0196da",
            "foreground": "#f2f3f8",
            "properties": {
              "style": "agnoster_short",
              "max_depth": 3,
              "folder_icon": "\u2026 ",
              "folder_separator_icon": " <transparent>\ue0b1</> "
            },
            "style": "powerline",
            "powerline_symbol": "\ue0b0",
            "template": " {{ .Path }} ",
            "type": "path"
          },
          {
            "background": "#378504",
            "foreground": "#f2f3f8",
            "background_templates": [
              "{{ if or (.Working.Changed) (.Staging.Changed) }}#a97400{{ end }}",
              "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#54433a{{ end }}",
              "{{ if gt .Ahead 0 }}#744d89{{ end }}",
              "{{ if gt .Behind 0 }}#744d89{{ end }}"
            ],
            "properties": {
              "branch_max_length": 25,
              "fetch_stash_count": true,
              "fetch_status": true,
              "branch_icon": "\uf418 ",
              "branch_identical_icon": "\uf444",
              "branch_gone_icon": "\ueab8"
            },
            "style": "diamond",
            "leading_diamond": "<transparent,background>\ue0b0</>",
            "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#121318>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#121318>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#121318>\ueb4b {{ .StashCount }}</>{{ end }} ",
            "trailing_diamond": "\ue0b0",
            "type": "git"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "right",
        "segments": [
          {
            "background": "#e4e4e4",
            "foreground": "#585858",
            "properties": {
              "style": "austin",
              "always_enabled": true
            },
            "invert_powerline": true,
            "style": "powerline",
            "powerline_symbol": "\ue0b2",
            "template": " \ueba2 {{ .FormattedMs }} ",
            "type": "executiontime"
          },
          {
                      "background": "#fec810",
                      "foreground": "#100e23",
            "invert_powerline": true,
            "style": "powerline",
            "powerline_symbol": "\ue0b2",
            "template": " \uf007 {{ if matchP .UserName \"/dillo/\" }}Dillon {{else}}{{ .UserName }} {{end}}@ {{ replace \"Dillons-\" \"\" .HostName }} ",
                      "type": "session"
                  },
          {
            "background": "#0196da",
            "foreground": "#f2f3f8",
            "properties": {
              "time_format": "03:04 PM"
            },
            "invert_powerline": true,
            "style": "diamond",
            "template": " \ue641 {{ .CurrentDate | date .Format }} ",
            "trailing_diamond": "\ue0b0",
            "type": "time"
          }
        ],
        "type": "prompt"
      },
      {
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "foreground": "#fec810",
            "style": "plain",
            "template": "{{ if .Root }}\uea86 {{else}}\ue683 {{end}}",
            "type": "text"
          }
        ],
        "type": "prompt"
      }
    ],
      "final_space": true,
    "version": 2
  }
